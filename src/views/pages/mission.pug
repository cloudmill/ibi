extends ../layouts/master



block vars


  
block content
  .mission
    section.section.section--gradient
      .row
        .row__col
          .section__inner.section__inner--top
            .nav
              .nav__container
                nav.nav__nav(data-sticky-id='1' data-sticky-top='250' data-sticky-bottom='120')
                  ul.nav__list
                    li.nav__item
                      a.nav__link(href='#')
                        |О нас
                    li.nav__item.nav__item--active
                      a.nav__link(href='#')
                        |Миссия и ценности
                    each item in ['Наша команда', 'Новости', 'Контакты']
                      li.nav__item
                        a.nav__link(href='#')
                          |#{item}
        .row__col
          .section__inner.section__inner--top
            .container
              h1.title
                |Миссия и ценности
              .section__block.section__block--small
                .mission__missions-title
                  |Миссия
                ul.mission__missions-list
                  li.mission__missions-item
                    .mission__missions-text.mission__missions-text--max-width--730
                      |Делать жизнь 
                      span.mission__missions-text-highlight
                        |комфортной и полноценной
                      |#{' '}с помощью базальной имплантации.
                    ul.mission__missions-links
                      li.mission__missions-links-item
                        .mission__missions-links-container
                          a.mission__missions-links-link(href='#')
                            +svg-link.mission__missions-links-svg
                            .mission__missions-links-text
                              |О методе
                      li.mission__missions-links-item
                        .mission__missions-links-container
                          a.mission__missions-links-link(href='#')
                            +svg-link.mission__missions-links-svg
                            .mission__missions-links-text
                              |Приемущества метода
                    ul.mission__missions-images.mission__missions-images--top
                      li.mission__missions-image
                        img.mission__missions-img(src='assets/images/mission-1.jpeg' alt='')
                      li.mission__missions-image
                        img.mission__missions-img(src='assets/images/mission-2.jpeg' alt='')
