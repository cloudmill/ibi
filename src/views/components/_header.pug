mixin header
  header.header
    .header__header
      .container
        .header__inner
          .header__row
            .header__col
              .header__container
                .header__logo
                  a.header__logo-link(href='#')
                    +svg-logo.header__logo-svg
            .header__col
              .header__panel
                .header__panel-item.header-menu
                  .header__items.header__container
                    .header__items-item
                      .header__drop
                        .header__drop-button(data-modal-button='1')
                          .header__drop-text.header__text
                            |Базальная имплантация
                          .header__drop-image
                            +svg-arrow.header__drop-svg
                    .header__items-item
                      .header__drop
                        .header__drop-button(data-modal-button='2')
                          .header__drop-text.header__text
                            |Услуги
                          .header__drop-image
                            +svg-arrow.header__drop-svg
                    .header__items-item
                      a.header__link.header__text(href='#')
                        |До и После
                .header__panel-item
                  .header__items.header__items--large.header__container
                    .header__items-item
                      .header__phone
                        a.header__phone-call.header__text.callback-button(href='#')
                          |Заказать звонок
                        a.header__phone-link.header__text.header__link(href='tel:84994906349')
                          |8 499 490 63 49
                    .header__items-item.header-lang-drop
                      .header__drop
                        .header__drop-button
                          .header__drop-text.header__text
                            |Ru
                          .header__drop-image
                            +svg-arrow.header__drop-svg
                    .header__items-item.header__items-item--mobile
                      .header__button
                        .header__button-button(data-modal-button='3')
                          .header__button-image
    .header__modals
